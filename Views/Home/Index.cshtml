@{
    ViewData["Title"] = "Index";
}

<h1>ROPA</h1>
<div>
    <h1><a href='@Url.Action("SelectIndumentaria")'>Select indumentaria</a></h1>
    <ul>
        @foreach (string equipo in ViewBag.EquiposIndumentaria.Keys)
        {
            <div class="div1">
                <p>@equipo</p>
                <img src="/IMG/CAM/@ViewBag.EquiposIndumentaria[equipo].Camiseta">
                <img src="/IMG/CAM/PANT/@ViewBag.EquiposIndumentaria[equipo].Pantalon">
                <img src="/IMG/CAM/MED/@ViewBag.EquiposIndumentaria[equipo].Medias">
                <br>
            </div>
        }
    </ul>
</div>